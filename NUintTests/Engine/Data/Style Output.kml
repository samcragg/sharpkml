          <altitudeMode>relativeToGround</altitudeMode>
        </LookAt>
        <StyleMap>
          <Pair>
            <key>normal</key>
            <Style>
              <IconStyle>
                <scale>1.1</scale>
                <Icon>
                  <href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
                </Icon>
                <hotSpot x="20" xunits="pixels" y="2" yunits="pixels"/>
              </IconStyle>
            </Style>
          </Pair>
          <Pair>
            <key>highlight</key>
            <Style>
              <IconStyle>
                <scale>1.3</scale>
                <Icon>
                  <href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
                </Icon>
                <hotSpot x="20" xunits="pixels" y="2" yunits="pixels"/>
              </IconStyle>
            </Style>
          </Pair>
        </StyleMap>
        <Point>
          <coordinates>12.487995,47.969918</coordinates>
        </Point>
      </Placemark>
    </Document>
